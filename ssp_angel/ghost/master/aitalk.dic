//Pick a new fortune each day, which is unique to this user
//Use the user's username and birthday to generate an "offset", then use the current date to pick a random number to add to this offset
//When combined, this generates a random output that stays consistent throughout the day, and is specific to this user (unless someone shares the same username and birthday, in which case, twinsies!)
OnTodaysFortune
{
	//Just a couple shorthands to avoid duplicated code below
	_fortune_size = ARRAYSIZE(fortunes)
	_byear = SHIORI3FW.UserBirthday[0]
	_bmonth = SHIORI3FW.UserBirthday[1]
	_bday = SHIORI3FW.UserBirthday[2]
	
	_hash = STRDIGEST(username)
	_hash = SUBSTR(_hash,0,9) //Any more and we start going over the 64 bit integer limit...
	_hash = HEXSTRTOI(_hash)
	
	_userseed = TOSTR(_hash) + TOSTR(_byear) + TOSTR(_bmonth) + TOSTR(_bday)
	_dayseed =  TOSTR(year) + TOSTR(month) + TOSTR(day)
	
	void SRAND(TOINT(_userseed))
	_offset = RAND(_fortune_size)
	
	void SRAND(TOINT(_dayseed))
	_daypick = RAND(_fortune_size)
	
	_i = (_offset + _daypick) % _fortune_size
	
	_fortune = fortunes[_i]
	
	//"\_qToday's fortune: %(_fortune)\nUser seed: %(_userseed) Offset: %(_offset)\nDay seed: %(_dayseed)  Day pick: %(_daypick)\nDraw: %(_i)\nAvail: %(_fortune_size)"
	
